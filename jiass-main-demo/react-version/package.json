{
  "name": "ads-power-clone-react",
  "version": "1.0.0",
  "description": "A React 18 clone of AdsPower fingerprint browser management software",
  "main": "src/main.js",
  "scripts": {
    "start": "npm run build && ELECTRON_DISABLE_SECURITY_WARNINGS=true ELECTRON_NO_ATTACH_CONSOLE=true electron .",
    "dev": "npm run build && ELECTRON_DISABLE_SECURITY_WARNINGS=true ELECTRON_NO_ATTACH_CONSOLE=true electron . --dev",
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.config.js --watch",
    "pack": "echo 'Pack completed'",
    "dist": "echo 'Dist completed'"
  },
  "keywords": [
    "electron",
    "react",
    "fingerprint",
    "browser",
    "automation"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "css-loader": "^6.8.0",
    "electron": "^25.0.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {
    "electron-store": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
